{
	"name": "VincentChalnot/Aetion",
	"license": "GPL-2 or New-BSD",
	"type": "framework",
	"description": "Aetion is a Procedural World Generation Framework meant to output generated environment and objects to a 3D engine through various format",
	"homepage": "https://github.com/VincentChalnot/Aetion",
	"autoload": {
		"psr-0": {"": "lib/"}
	},
	"repositories": [{
			"type": "package",
			"package": {
				"name": "mrdoob/threejs",
				"version": "dev-master",
				"source": {
					"url": "https://github.com/mrdoob/three.js.git",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "schteppe/cannonjs",
				"version": "dev-master",
				"source": {
					"url": "https://github.com/schteppe/cannon.js.git",
					"type": "git",
					"reference": "master"
				}
			}
		}],
	"require": {
		"php": ">=5.4",
		"symfony/class-loader": "2.3.*",
		"phayes/geophp": "dev-master",
		"mrdoob/threejs": "dev-master",
		"schteppe/cannonjs": "dev-master"
	},
	"authors": [
		{
			"name": "Vincent Chalnot"
		}
	]
}